{% extends 'base.html.twig' %}

{% block title %}Realisation
{% endblock %}

{% block body %}
	<h2 class="h2titrepage">Réalisation</h2>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ realisation.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ realisation.name }}</td>
			</tr>
			<tr>
				<th>Image</th>
				<td>{{ realisation.image }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ realisation.description }}</td>
			</tr>
		</tbody>
	</table>

	<a class="bouton" href="{{ path('app_admin_index') }}" title="retour">Retour à la liste</a>

	<a class="btn btn-warning" href="{{ path('app_realisation_edit', {'id': realisation.id}) }}" title="Modifier">Modifier</a>

	{{ include('realisation/_delete_form.html.twig') }}
{% endblock %}
